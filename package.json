{
  "name": "descriptions-ui",
  "version": "1.33.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode development",
    "dev:dev": "vite --mode dev",
    "dev:test": "vite --mode test",
    "dev:staging": "vite --mode staging",
    "dev:prod": "vite --mode prod",
    "build": "concurrently \"npm run type-check\" \"npm run build-only\"",
    "build:local": "concurrently \"npm run type-check\" \"npm run build-only\"",
    "build:dev": "concurrently \"npm run type-check\" \"npm run build-only:dev\"",
    "build:ci": "concurrently \"npm run type-check\" \"npm run build-only:ci\"",
    "build:test": "concurrently \"npm run type-check\" \"npm run build-only:test\"",
    "build:staging": "concurrently \"npm run type-check\" \"npm run build-only:staging\"",
    "build:prod": "concurrently \"npm run type-check\" \"npm run build-only:prod\"",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom",
    "test:unit-with-coverage": "vitest --environment jsdom --coverage",
    "test:unit-with-coverage-ci": "vitest --environment jsdom --coverage --run",
    "test:unit-with-coverage-default": "vitest --environment jsdom --coverage --reporter=default",
    "test:unit-with-coverage-junit": "vitest --environment jsdom --coverage --reporter=junit",
    "test:unit-ci": "vitest --environment jsdom --coverage --run --reporter=default --reporter=junit",
    "test:e2e": "npx playwright test --ui --project=chromium",
    "validate": "npm run lint && npm run build && npm run test:unit",
    "validate:parallel": "concurrently \"npm run lint\" \"npm run build\" \"npm run test:unit\"",
    "build-only": "vite build --mode development",
    "build-only:dev": "vite build --mode dev",
    "build-only:ci": "vite build --mode ci",
    "build-only:test": "vite build --mode test",
    "build-only:staging": "vite build --mode staging",
    "build-only:prod": "vite build --mode prod",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "npm run lint:oxlint && npm run lint:eslint",
    "format": "prettier .  --write"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.13.0",
    "@bestsecret/design-system": "^0.3.8",
    "@mdi/font": "^7.4.47",
    "@vue/language-core": "^2.2.10",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "js-cookie": "^3.0.5",
    "nyc": "^15.1.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.3.0",
    "rollbar": "^2.26.4",
    "sanitize-html": "^2.17.0",
    "sass": "^1.89.1",
    "vue": "^3.5.16",
    "vue-i18n": "^11.1.5",
    "vue-router": "^4.5.1",
    "vue-toastification": "^2.0.0-rc.2",
    "vuetify": "^3.8.8"
  },
  "devDependencies": {
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@pinia/testing": "^1.0.2",
    "@playwright/test": "1.52.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/js-cookie": "^3.0.6",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.30",
    "@types/sanitize-html": "^2.16.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.2",
    "@vitest/eslint-plugin": "^1.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.2",
    "cross-fetch": "^4.1.0",
    "eslint": "^9.28.0",
    "eslint-plugin-oxlint": "^0.18.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.2.0",
    "flush-promises": "^1.0.2",
    "glob": "^11.0.2",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "oxlint": "^0.18.0",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "start-server-and-test": "^2.0.12",
    "test-exclude": "^7.0.1",
    "typescript": "^5.8.3",
    "vite": "6.3.2",
    "vite-plugin-css": "^1.0.4",
    "vite-plugin-istanbul": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitest": "^3.2.2",
    "vitest-fetch-mock": "^0.4.5",
    "vue-tsc": "^2.2.10"
  },
  "overrides": {
    "glob": "^11.0.2",
    "rimraf": "^6.0.1",
    "inflight": "npm:lru-cache@^7.14.1",
    "express": "^4.18.3"
  }
}
